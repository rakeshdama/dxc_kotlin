<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans  
               http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="studentbean" class="first_spring.Student">
		<property name="name" value="Rakesh Dama"></property>
	</bean>


	<bean id="employee" class="first_spring.Employee">
		<constructor-arg value="21" type="int"></constructor-arg>
		<constructor-arg value="Rakesh Dama"></constructor-arg>
	</bean>

	<bean id = "q" class = "constructor.injection.Question">
	
	<constructor-arg value = "18"></constructor-arg>
	<constructor-arg value = "Rakesh Dama"></constructor-arg>
	<constructor-arg>
	
	<list>
	
	<value>Rakesh Royal</value>
	<value>Hacker</value>
	<value>Indian Top Hacker</value>
	
	</list>
	
	</constructor-arg>
	
	</bean>
</beans> 